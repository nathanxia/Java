
import java.time.*;





	public class Parent {
	    protected static int count = 0;
	    public Parent() { count++; }
	    static int getCount() { return count; }
	}

	public class test extends Parent {
	    public test() { count++; }
	    public static void main(String [] args) {
	        System.out.println("Count = " + getCount());
	        test obj = new test();
	        System.out.println("Count = " + getCount());
	    }
	}

	
	
	
	

