/*
 * 练习：
 * 创建两个线程，和主线程交替运行。
 */

class Test extends Thread {
	
	public void run() {
		for (int i = 0; i < 50; i++) {
			System.out.println("Thread " + this.getName() + " -> " + i);
		}
	}
}

public class L02_ThreadTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Test t1 = new Test("1");
		Test t2 = new Test("2");
		
		t1.start();
		t2.start();
		
		for (int i = 0; i < 50; i++) {
			System.out.println("Main -> " + i);
		}
	}

}
