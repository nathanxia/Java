
public class intByteTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x = 1;
		int y = 4;
		
		int count = 0;
        int n = x ^ y;
        System.out.println(n);
        while (n != 0) {
            count++;
            n = n & (n - 1);
            System.out.println(n);
        }
		
		
	}

}
