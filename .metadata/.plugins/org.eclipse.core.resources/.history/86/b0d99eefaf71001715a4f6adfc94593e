	public static String reverse(String[] arr, int i) {
		
		if(i == arr.length - 1) {
			return arr[i];
		}
		System.out.println("|"+arr[i]+"|");
		String ss = reverse(arr, i + 1);
		if (!arr[i].equals("")) {
			ss = ss + " " + arr[i];
		}
		return ss;
	}

	public static void display(char[] arr) {
		System.out.print("| ");
		for (char val : arr) {
			System.out.print(val + " | ");
		}
		System.out.println();
	}
}
