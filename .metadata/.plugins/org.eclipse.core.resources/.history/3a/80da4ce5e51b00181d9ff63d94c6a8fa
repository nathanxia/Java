class Bank {
	private int sum;
	
	public void add(int n) {
		sum += n;
		System.out.println("Sum = " + sum);
	}
}

class Customer implements Runnable {
	
	private Bank b = new Bank();
	
	public void run() {
		
		for (int i = 0; i < 3; i++) {
			b.add(100);
		}
	}
	
}
public class L05_BankDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
